{
    "name": "etu/etu-slim-skeleton",
    "description": "An opinionated template for PHP projects using Slim Framework 4 that has some care and thought put into error handling and logging",
    "homepage": "https://github.com/etu/etu-slim-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Elis Hirwing",
            "email": "elis@hirwing.se",
            "homepage": "https://elis.nu/"
        }
    ],
    "require": {
        "php": "^8.0",
        "etu/slim-libs": "^0.0.1",
        "bramus/monolog-colored-line-formatter": "^3.0",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
